{
  "name": "DSSAT",
  "workDir": "/tmp/results",
  "templateDir": "/data/kenya/templates",
  "weatherDir": "/data/kenya/weather",
  "threads": 2,
  "cores": 4,
        "sample": null,
  "ghr_root":"/data/base/eGHR",
  "plugins":[{
            "plugin": "sensitivity_plugin",
            "params": {
                "fen_tot": {
                    "method": "offset",
                    "values": [0]
                },
                "erain": {
                    "method": "env_mod",
                    "values": [1.0]
                },
                "pfrst": {
                    "method": "date_offset",
                    "values": [0],
                    "hook": "post_build_context"
                },
                "plast": {
                    "comment": "Planting window is 42 days (or 6 weeks) long",
                    "method": "date_offset",
                    "value": 42,
                    "from": "pfrst",
                    "hook": "post_build_context",
                    "static": true
                }
            },
            "order": 1,
            "no_rename": true
        }],
  "default_setup": {
    "include":["/data/kenya/includes/MZCER047.CUL"],
    "template": "CRMZ8401.SNX",
    "sites": "xy_from_vector::/data/kenya/shapes/KEN_five_arc_points.shp",
    "startYear": 1984,
    "nyers": 37,
                "sdate": "1984-01-01",
    "icin": 10,
    "icsw%": 25,
    "flhst":"FH302",
    "fhdur": 10,
    "icren": 0.8,
    "erain": 1.00,
    "ph2ol": 40,
                "ingeno": "IC0001",
                "cname": "H511",
    "id_soil": "lookup_ghr::raster::/data/base/eGHR/GHR.tif",
    "wsta": "lookup_wth::GGCP::vector::/data/kenya/shapes/KEN_five_arc_points.shp::ID",
    "ic_layers": "generate_ic_layers::$id_soil",
    "fertilizers": "split_fert_dap_percent::$fen_tot::5::0::20::30::80"
  },
  "dssat": {
    "executable": "/opt/dssat/run_dssat"
  },
  "analytics_setup": {
    "per_pixel_prefix":"pp"
  },
  "runs": [
    {
      "name": "Irrigated",
      "harvestArea": "raster::/data/base/spam2010/spam2010V2r0_global_H_MAIZ_I.tif",
      "icres": 200,
      "icrt": 20,
      "fen_tot": 100,
      "ramt": 250,
      "icsw%": 25,
      "irrig": "A",
      "pfrst": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PStart_SAGE.tif",
      "plast": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PMid_SAGE.tif"
    },
    {
      "name": "Rainfed-high_N_fertilizer",
      "harvestArea": "raster::/data/base/spam2010/spam2010V2r0_global_H_MAIZ_H.tif",
      "icres": 200,
      "icrt": 20,
      "fen_tot": 50,
      "ramt": 250,
      "icsw%": 25,
      "irrig": "R",
      "pfrst": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PStart_SAGE.tif",
      "plast": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PMid_SAGE.tif"
    },
    {
      "name": "Rainfed-low_N_fertilizer",
      "harvestArea": "raster::/data/base/spam2010/spam2010V2r0_global_H_MAIZ_L.tif",
      "icres": 200,
      "icrt": 20,
      "fen_tot": 25,
      "ramt": 50,
      "icsw%": 25,
      "irrig": "R",
      "pfrst": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PStart_SAGE.tif",
      "plast": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PMid_SAGE.tif"
    },
    {
      "name": "Rainfed-no_N_fertilizer",
      "harvestArea": "raster::/data/base/spam2010/spam2010V2r0_global_H_MAIZ_S.tif",
      "icres": 200,
      "icrt": 20,
      "fen_tot": 0,
      "ramt": 25,
      "icsw%": 25,
      "irrig": "R",
      "pfrst": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PStart_SAGE.tif",
      "plast": "date_from_doy_raster::raster::/data/base/SAGEcropcalendar/MZ_PMid_SAGE.tif"
    }

  ]
}

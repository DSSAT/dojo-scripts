{
  "name": "DSSAT",
  "workDir": "/tmp/results",
  "templateDir": "/data/ethiopia/templates",
  "weatherDir": "/data/ethiopia/weather",
  "threads": 2,
  "cores": 4,
  "ghr_root": "/data/base/eGHR",
  "sample": 10,
  "plugins": [
    {
      "plugin": "sensitivity_plugin",
      "params": {
	"fen_tot": {
	  "method": "offset",
	  "values": [
	    0
	  ]
	},
	"erain": {
	  "method": "env_mod",
	  "values": [
	    1
	  ]
	},
	"pfrst": {
	  "method": "date_offset",
	  "values": [
	    0
	  ],
	  "hook": "post_build_context"
	},
	"plast": {
	  "comment": "Planting window is 42 days (or 6 weeks) long",
	  "method": "date_offset",
	  "value": 42,
	  "from": "pfrst",
	  "hook": "post_build_context",
	  "static": true
	},
	"sdate": {
	  "comment": "Start date is set to 180 days before planting date",
	  "method": "date_offset",
	  "value": -180,
	  "from": "pfrst",
	  "hook": "post_build_context",
	  "static": true
	}
      },
      "order": 1,
      "no_rename": true
    }
  ],
  "default_setup": {
    "include": [
      "/data/ethiopia/includes/MZCER048.CUL"

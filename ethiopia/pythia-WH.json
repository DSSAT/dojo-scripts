{
  "name": "DSSAT",
  "workDir": "/tmp/results",
  "templateDir": "/data/ethiopia/templates",
  "weatherDir": "/data/ethiopia/weather",
  "threads": 4,
  "cores": 8,
  "ghr_root": "/data/base/eGHR",
  "plugins": [
    {
      "plugin": "sensitivity_plugin",
      "params": {
        "fen_tot": {
          "method": "offset",
          "values": [
            ~f_i~
          ]
        },
        "erain": {
          "method": "env_mod",
          "values": [
            "M~p_m~"
          ]
        },
        "pfrst": {
          "method": "date_offset",
          "values": [
            ~pd_s~
          ],
          "hook": "post_build_context"
        },
        "plast": {
          "comment": "Planting window is 42 days (or 6 weeks) long",
          "method": "date_offset",
          "value": 42,
          "from": "pfrst",
          "hook": "post_build_context",
          "static": true
        },
        "sdate": {
          "comment": "Start date is set to 180 days before planting date",
          "method": "date_offset",
          "value": -180,
          "from": "pfrst",
          "hook": "post_build_context",
          "static": true
        }
      },
      "order": 1,
      "no_rename": true
    }
  ],
  "default_setup": {
    "include": [
      "/data/ethiopia/includes/WHCER048.CUL"
    ],
    "template": "DSWH2201.SNX",
    "sites": "xy_from_vector::/data/ethiopia/shapes/Five_arcmin_pt_Eth.shp",
    "nyers": 35,
    "icin": 10,
    "icsw%": 25,
    "flhst": "FH302",
    "fhdur": 10,
    "icren": 0.8,
    "pfrst": "assign_by_raster_value::raster::/data/ethiopia/rasters/New_eth_pdatzones.tif::1::1985-07-01::2::1985-07-15::3::1985-10-20::4::1985-07-25",
    "plast": "assign_by_raster_value::raster::/data/ethiopia/rasters/New_eth_pdatzones.tif::1::1985-08-15::2::1985-08-31::3::1985-11-30::4::1985-08-31",
    "sdate": "assign_by_raster_value::raster::/data/ethiopia/rasters/New_eth_pdatzones.tif::1::1985-01-01::2::1985-01-15::3::1985-04-20::4::1985-01-25",
    "id_soil": "lookup_ghr::raster::/data/ethiopia/rasters/GHR.tif",
    "wsta": "lookup_wth::ETCP::vector::/data/ethiopia/shapes/Five_arcmin_pt_Eth.shp::ID",
    "ic_layers": "generate_ic_layers::$id_soil",
    "ramt": 0,
    "fertilizers": "split_fert_dap_percent::$fen_tot::5::0::50::30::50",
    "population": "raster::/data/base/gpw/gpw_v4_population_count_adjusted_to_2015_unwpp_country_totals_rev11_2020_agg5_min.tif"
  },
  "dssat": {
    "executable": "/opt/dssat/dscsm048"
  },
  "analytics_setup": {
    "singleOutput": true
  },
  "runs": [
